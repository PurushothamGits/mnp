pipeline {
    agent any
    stages {
        stage('Git clone') {
            steps {
                checkout scm
            }
        }
        stage('Deployment') {
            steps {
                script {
                   sh '''
                   sudo cp /var/lib/jenkins/workspace/${JOB_NAME}/purushotham.html /var/www/html
                    '''
                }
            }
        }
    }
}
